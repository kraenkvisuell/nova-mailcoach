{
    "name": "kraenkvisuell/nova-mailcoach",
    "description": "Mailcoach Laravel Nova",
    "keywords": [
        "laravel",
        "nova",
        "layout"
    ],
    "license": "MIT",
    "repositories": [
        {
            "type": "composer",
            "url": "https://nova.laravel.com"
        },
        {
            "type": "composer",
            "url": "https://satis.mailcoach.app"
        }
    ],
    "require": {
        "eminiarts/nova-tabs": "^1.2",
        "manogi/nova-tiptap": "^0.6.1",
        "optimistdigital/nova-media-field": "^1.2",
        "whitecube/nova-flexible-content": "^0.2.5"
    },
    "autoload": {
        "psr-4": {
            "Kraenkvisuell\\NovaMailcoach\\": "src/"
        },
        "files": [
            "src/Helpers/functions.php"
        ]
    },
    "autoload-dev": {
        "psr-4": {
            "Kraenkvisuell\\NovaMailcoach\\Tests\\": "tests"
        }
    },
    "config": {
        "sort-packages": true
    },
    "minimum-stability": "dev",
    "prefer-stable": true,
    "require-dev": {
        "laravel/nova": "~3.0",
        "orchestra/testbench": "^5.3",
        "phpunit/phpunit": "^9.2",
        "spatie/laravel-mailcoach": "^2.0.0"
    },
    "extra": {
        "laravel": {
            "providers": [
                "Kraenkvisuell\\NovaMailcoach\\NovaMailcoachServiceProvider"
            ]
        }
    }
}
